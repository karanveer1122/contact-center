{
    "version" : "2017-02-28",
    "operation" : "UpdateItem",
    "key" : {
        "agentId" : { "S" : "${context.arguments.agentId}" }
    },
    "update" : {
        "expression" : "SET #s = :s, #e = :e, #st = :st",
        "expressionNames": {
            "#s" : "startTimeStamp",
            "#st" : "status",
            "#e" : "endTimeStamp"
        },
        "expressionValues": {
            ":s" : $util.dynamodb.toDynamoDBJson($ctx.args.startTimeStamp),
            ":e" : $util.dynamodb.toDynamoDBJson($ctx.args.endTimeStamp),
            ":st" : $util.dynamodb.toDynamoDBJson($ctx.args.status)     
        }
    }
}